<div id="main" class="<%= item.layout %>" itemscope itemprop="blogPost">
	<article itemprop="articleBody"> 
		<%- partial('header') %>
		<div id="google_translate_element"></div>
		<script type="text/javascript">
			function googleTranslateElementInit() {
				new google.translate.TranslateElement({pageLanguage: 'en', includedLanguages: 'en,ja,zh-TW'}, 'google_translate_element');
			}
		</script>
		<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
		<div class="article-content">
			<%- partial('gallery') %>
			<% if( table&&(item.toc !== false) && theme.toc.article){ %>
			<div id="toc" class="toc-article">
				<strong class="toc-title"><%= __('contents') %></strong>
				<% if(item.list_number == false) {%>
				<%- toc(item.content,{list_number:false}) %>
				<% }else{ %>
				<%- toc(item.content) %>
				<% } %>
			</div>
			<% } %>
			<%- item.content %>  
		</div>
		<%- partial('footer') %>   	       
	</article>
	<%- partial('pagination') %>
	<%- partial('comment') %>
</div>  